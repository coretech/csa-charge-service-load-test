{
  "name": "csa-analytics-service-load-test",
  "description": "CSA Rate Services Load Tests",
  "repository": "https://github.com/coretech/csa-analytics-service-load-test.git",
  "private": true,
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/faker": "5.5.9",
    "@types/k6": "0.37.0",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "8.2.4",
    "babel-plugin-tsconfig-paths": "1.0.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "5.5.3",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-glob-entries": "1.0.1",
    "papaparse": "5.3.2"
  },
  "scripts": {
    "start:containers": "docker-compose up -d influxdb grafana",
    "stop:containers": "docker-compose stop",
    "delete:containers": "docker-compose down -v",
    "clean": "rimraf ./dist",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "prettier": "prettier --check src/**/*.ts",
    "check:code": "npm run lint && npm run prettier",
    "bundle": "webpack",
    "execute:tests": "docker-compose run -e K6_INFLUXDB_USERNAME=$npm_config_influxdb_user -e K6_INFLUXDB_PASSWORD=$npm_config_influxdb_pass k6 run -e BASE_URL=$npm_config_base_url /dist/$npm_config_script_name.js",
    "execute:tests:local": "docker-compose run k6_local run -e BASE_URL=$npm_config_base_url /dist/$npm_config_script_name.js"
  }
}
